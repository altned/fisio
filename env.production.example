# Fisioku Production Environment
NODE_ENV=production
PORT=3000

# Database - PostgreSQL on host
DATABASE_URL=postgresql://fisioku:YOUR_DB_PASSWORD@host.docker.internal:5432/fisioku

# JWT Secret - CHANGE THIS!
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Midtrans Payment Gateway
MIDTRANS_SERVER_KEY=your-midtrans-server-key
MIDTRANS_CLIENT_KEY=your-midtrans-client-key
MIDTRANS_IS_PRODUCTION=false

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id

# CORS - allowed origins
ALLOWED_ORIGINS=https://api.fisioku.id,https://admin.fisioku.id,https://fisioku.id

# Redis
REDIS_URL=redis://fisioku-redis:6379
